extends layout

block content
  .container
    h2 电影列表
    each item in movies
      .col-md-2
        .thumbnail
          a(href="#")
            img(src="#{item.img}",alt="#{item.name}", name="#{item.id}")
          .caption
            h4 #{item.name}
  script(src="/moment/moment.js")
  script(src="/javascripts/index.js")
