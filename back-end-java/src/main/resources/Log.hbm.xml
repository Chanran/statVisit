<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="hibernate.Log" table="log" schema="stat">
        <id name="id">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="ip"/>
        <property name="log_text"/>
        <property name="time"/>
    </class>
</hibernate-mapping>